{
  "dxt_version": "0.2",
  "name": "vra_iaas-mcp",
  "version": "0.0.3",
  "description": "The official MCP Server for the Vra Iaas API",
  "author": {
    "name": "Vra Iaas",
    "email": "sales@vmware.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imtrinity94/vra-iaas-mcp-typescript.git"
  },
  "homepage": "https://github.com/imtrinity94/vra-iaas-mcp-typescript/tree/main/packages/mcp-server#readme",
  "documentation": "https://www.vmware.com/",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "VRA_IAAS_BEARER_TOKEN": "${user_config.VRA_IAAS_BEARER_TOKEN}"
      }
    }
  },
  "user_config": {
    "VRA_IAAS_BEARER_TOKEN": {
      "title": "bearer_token",
      "description": "",
      "required": true,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
